{
  "EnableGC": true,
  "Deadline": "2m",
  "DisableAll": true,
  "Enable": [
    "deadcode",
    "gocyclo",
    "gofmt",
    "goimports",
    "golint",
    "gosimple",
    "ineffassign",
    "interfacer",
    "lll",
    "misspell",
    "unconvert",
    "unparam",
    "unused",
    "vet"
  ],
  "Exclude": [
    "pkg/source/source_test.go",
    "pkg/storage/storage_test.go"
  ],
  "Cyclo": 20,
  "LineLength": 220
}